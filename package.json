{
  "name": "curso-de-webpack",
  "version": "1.0.0",
  "description": "Este es un curso muy divertido",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack index.js bundle.js",
    "build:local": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:extract:css": "webpack --config ./plugin-extract-text/webpack.config.js",
    "build:multi": "webpack --config ./multiple-entry-points/webpack.config.js",
    "build:dev": "webpack --config ./webpack-dev-server/webpack.config.js --watch",
    "build:server": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js",
    "build:images": "webpack --config ./url-loader-images/webpack.config.js",
    "build:fonts": "webpack --config ./url-loader-fonts/webpack.config.js",
    "build:video": "webpack --config ./url-loader-video/webpack.config.js",
    "build:sass-loader": "webpack --config ./sass-loader/webpack.config.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "@eferreyramontes",
  "license": "MIT",
  "devDependencies": {
    "css-loader": "0.28.11",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "node-babel": "^0.1.2",
    "node-sass": "^4.9.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "sass-loader": "^7.0.3",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.9.7"
  },
  "dependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2016": "6.24.1"
  }
}
