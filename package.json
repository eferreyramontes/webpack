{
  "name": "curso-de-webpack",
  "version": "1.0.0",
  "description": "Este es un curso muy divertido",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack index.js bundle.js",
    "build:local": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:extract:css": "webpack --config ./plugin-extract-text/webpack.config.js",
    "build:multi": "webpack --config ./multiple-entry-points/webpack.config.js",
    "build:dev": "webpack --config ./webpack-dev-server/webpack.config.js --watch",
    "build:server": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "@eferreyramontes",
  "license": "MIT",
  "devDependencies": {
    "css-loader": "0.28.11",
    "extract-text-webpack-plugin": "3.0.2",
    "style-loader": "0.21.0",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.7"
  }
}
